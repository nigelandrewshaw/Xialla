{% layout 'collection' %}
      
  <div data-collapse="medium" data-animation="default" data-duration="400" role="banner" class="navigation w-nav">
    <div class="mainmenuwrap"><a href="/" id="w-node-f20facedcda8-cd55b9ef" class="brand w-nav-brand"><img src="{{ 'logo.png' | asset_url }}" width="44" alt="Xialla Logo" class="header-logo"></a>
      <div id="w-node-f20facedcdaa-cd55b9ef" class="nav1-menu-wrap">
        <div class="div-block-141">
          <nav role="navigation" class="nav-menu-2 w-nav-menu"><a href="#" class="nav-link-2 dropdown-home w-nav-link">{{ 'collection.A_0' | t }}</a>
            <div data-delay="0" class="nav-link-2 w-dropdown">
              <div class="dropdown-toggle w-dropdown-toggle">
                <div class="menu-text">{{ 'collection.DIV_1' | t }}</div>
                <div class="icon w-icon-dropdown-toggle"></div>
              </div>
              <nav class="dropdown-list-2 w-dropdown-list"><a href="/pages/how-it-works" class="menuitem dropdown w-dropdown-link">{{ 'collection.A_2' | t }}</a><a href="/pages/clinical-trials" class="menuitem dropdown w-dropdown-link">{{ 'collection.A_3' | t }}</a><a href="/pages/about" page="default" class="menuitem dropdown w-dropdown-link">{{ 'collection.A_4' | t }}</a><a href="/pages/contact" class="menuitem dropdown w-dropdown-link">{{ 'collection.A_5' | t }}</a></nav>
            </div>
            <div data-delay="0" class="nav-link-2 w-dropdown">
              <div class="dropdown-toggle w-dropdown-toggle">
                <div class="menu-text">{{ 'collection.DIV_6' | t }}</div>
                <div class="icon w-icon-dropdown-toggle"></div>
              </div>
              <nav class="dropdown-list-2 w-dropdown-list"><a href="/products/xialla" class="menuitem dropdown w-dropdown-link">{{ 'collection.A_7' | t }}</a><a href="/products/adhesive-pads-30-pack" class="menuitem dropdown w-dropdown-link">{{ 'collection.A_8' | t }}</a></nav>
            </div>
            <div data-delay="0" class="nav-link-2 w-dropdown">
              <div class="dropdown-toggle w-dropdown-toggle">
                <div class="menu-text">{{ 'collection.DIV_9' | t }}</div>
                <div class="icon w-icon-dropdown-toggle"></div>
              </div>
              <nav class="dropdown-list-2 w-dropdown-list"><a href="/pages/help" class="menuitem dropdown w-dropdown-link">{{ 'collection.A_10' | t }}</a><a href="/pages/customer-support" class="menuitem dropdown w-dropdown-link">{{ 'collection.A_11' | t }}</a><a href="/pages/instructions-for-use" class="menuitem dropdown w-dropdown-link">{{ 'collection.A_12' | t }}</a></nav>
            </div>
          </nav>
        </div>
        <div class="div-block-140">
          <div data-node-type="commerce-cart-wrapper" data-open-product="" data-wf-cart-type="rightDropdown" data-wf-cart-query="" data-wf-page-link-href-prefix="" class="w-commerce-commercecartwrapper cart-3">
            <a href="#" data-node-type="commerce-cart-open-link" class="w-commerce-commercecartopenlink button cc-cart w-inline-block">
              <div class="w-commerce-commercecartopenlinkcount cart-quantity-2" udy-el="cart-count">{{ cart.item_count }}</div>
            </a>
            <div data-node-type="commerce-cart-container-wrapper" style="display:none" class="w-commerce-commercecartcontainerwrapper w-commerce-commercecartcontainerwrapper--cartType-rightDropdown">
              <div data-node-type="commerce-cart-container" class="w-commerce-commercecartcontainer cart-container-3">
                <div class="w-commerce-commercecartheader">
                  <h4 class="w-commerce-commercecartheading">{{ 'collection.H4_13' | t }}</h4><a href="#" data-node-type="commerce-cart-close-link" class="w-commerce-commercecartcloselink close-button-3 w-inline-block"><svg width="16px" height="16px" viewBox="0 0 16 16"><g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd"><g fill-rule="nonzero" fill="#333333"><polygon points="6.23223305 8 0.616116524 13.6161165 2.38388348 15.3838835 8 9.76776695 13.6161165 15.3838835 15.3838835 13.6161165 9.76776695 8 15.3838835 2.38388348 13.6161165 0.616116524 8 6.23223305 2.38388348 0.616116524 0.616116524 2.38388348 6.23223305 8"></polygon></g></g></svg></a></div>
                <div class="w-commerce-commercecartformwrapper">
                  <form data-node-type="commerce-cart-form" class="w-commerce-commercecartform {% if cart.item_count == 0 %}udesly-display-none{% endif %}">
                    
                    
                    <ul role="list" class="mini-cart-list" udy-el="items-list">
                      {% for item in cart.items %}<li class="mini-cart-list-item">
                        <div class="product-row">
                          <div class="div-block-137"><img src="{{ item.product.featured_image.src | img_url: 'large' }}" alt="" class="image-32">
                            <div class="div-block-142">
                              <div class="cart-product-name">{{ item.product.title }}</div>
                              <div class="cart-product-price">{{ item.price | money }}</div>
                            </div>
                          </div>
                          <div>
                            <div class="text-block-82">{{ item.quantity }}</div>
                          </div>
                        </div>
                      </li>{% endfor %}
                    </ul>
                    <div class="w-commerce-commercecartfooter {% if cart.item_count == 0 %}udesly-display-none{% endif %}" udy-el="cart-actions">
                      <div class="w-commerce-commercecartlineitem popup-cart-line-item">
                        <div class="popup-cart-subtotal-label">{{ 'collection.DIV_14' | t }}</div>
                        <div class="w-commerce-commercecartordervalue popup-cart-subtotal" udy-el="cart-total">{{ cart.total_price | money }}</div>
                      </div>
                      <div class="checkout-actions"><a href="/cart" value="Edit" data-node-type="cart-checkout-button" class="w-commerce-commercecartcheckoutbutton submit-button-2 cart-details" data-loading-text="Edit">{{ 'collection.A_15' | t }}</a><a href="/checkout" value="Checkout" data-node-type="cart-checkout-button" class="w-commerce-commercecartcheckoutbutton submit-button-2 checkout" data-loading-text="Chekout">{{ 'collection.A_16' | t }}</a></div>
                    </div>
                  </form>
                  <div class="w-commerce-commercecartemptystate {% if cart.item_count != 0 %}udesly-display-none{% endif %}" udy-el="no-items-in-cart">
                    <div class="text-block-84">{{ 'collection.DIV_17' | t }}</div>
                  </div>
                  <div style="display:none" data-node-type="commerce-cart-error" class="w-commerce-commercecarterrorstate">
                    <div class="w-cart-error-msg" data-w-cart-quantity-error="Product is not available in this quantity." data-w-cart-checkout-error="Checkout is disabled on this site." data-w-cart-general-error="Something went wrong when adding this item to the cart." data-w-cart-cart_order_min-error="Cart failed.">{{ 'collection.DIV_18' | t }}</div>
                  </div>
                </div>
              </div>
            </div>
          <script id="mini-cart-template">
          window.udeslyMiniCartTemplate = function(item) {
            return `<li class="mini-cart-list-item" data-item-id="${item.id}">
                        <div class="product-row">
                          <div class="div-block-137"><img src="${item.image}" alt="" class="image-32">
                            <div class="div-block-142">
                              <div class="cart-product-name">${item.title}</div>
                              <div class="cart-product-price">${item.price}</div>
                            </div>
                          </div>
                          <div>
                            <div class="text-block-82">${item.quantity}</div>
                          </div>
                        </div>
                      </li>`
          }
          </script></div>
          <a href="/products/xialla" class="button-small header w-inline-block">
            <div class="button-label">{{ 'collection.DIV_19' | t }}</div>
          </a>
          <div class="menu-button-2 w-nav-button"><img src="{{ 'bars.svg' | asset_url }}" alt="" class="menu-icon-2"></div>
        </div>
      </div>
    </div>
  </div>
  <div class="section">
    <div class="container cc-heading-wrap">
      <h1>{{ collection.title }}</h1>
      <div class="store-tags-wrapper">{% for tag in collection.tags %}<a href="{% include 'collection-url' %}/{{ tag | handle }}" class="tags">{{ tag }}</a>{% endfor %}</div>
    </div>
  </div>
  <div class="section">
    {% paginate collection.products by settings.collection_products_limit %}<div class="w-dyn-list">
      {% unless collection.products == empty %}<div class="collection-wrap w-dyn-items">
        {% for product in collection.products %}<div class="collection-item w-dyn-item">
          <a href="{{ product.url }}" class="product-image-wrap w-inline-block" style="background-image: url({{ product.featured_image.src | img_url: 'master' }})">
            <div class="product-description">
              <div class="product-name-text">{{ product.title }}</div>
              <div class="product-price-block">
                <div class="product-price-text">{% if product.price_varies %}{{ product.price_min | money }} - {{ product.price_max | money }}{% else %}{{ product.price | money }}{% endif %}</div>
                {% if product.compare_at_price > product.price %}<div class="product-compare-price-text">{{ product.compare_at_price | money }}</div>{% endif %}
              </div>
            </div>
          </a>
        </div>{% endfor %}
      </div>
      {% else %}<div class="status-message cc-no-data w-dyn-empty">
        <div>{{ 'collection.DIV_20' | t }}</div>
      </div>{% endunless %}
      <div class="w-pagination-wrapper">{% if paginate.previous.is_link %}<a href="{{ paginate.previous.url }}" class="w-pagination-previous"><svg class="w-pagination-previous-icon" height="12px" width="12px" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 12 12" transform="translate(0, 1)"><path fill="none" stroke="currentColor" fill-rule="evenodd" d="M8 10L4 6l4-4"></path></svg><div class="w-inline-block">{{ 'collection.DIV_21' | t }}</div></a>{% endif %}
        {% if paginate.next.is_link %}<a href="{{ paginate.next.url }}" class="w-pagination-next">
          <div class="w-inline-block">{{ 'collection.DIV_22' | t }}</div><svg class="w-pagination-next-icon" height="12px" width="12px" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 12 12" transform="translate(0, 1)"><path fill="none" stroke="currentColor" fill-rule="evenodd" d="M4 2l4 4-4 4"></path></svg></a>{% endif %}
      </div>
    </div>{% endpaginate %}
  </div>
  <div class="section cc-subscribe-form">
    <div class="container cc-subscription-form">
      <div class="heading-jumbo-small">{{ 'collection.DIV_23' | t }}</div>
      <div class="paragraph-light cc-subscribe-paragraph">{{ 'collection.DIV_24' | t }}</div>
      {% form 'contact' %}<div class="form-block w-form">
        <div udesly-add-class="subscribe-form"></div><input type="email" class="text-field cc-subscribe-text-field w-input" maxlength="256" name="contact[email]" placeholder="Enter your email" id="email" required=""><input type="submit" value="Submit" data-wait="Please wait..." class="button w-button">
        
        
      </div><div class="form-block w-form">{% if form.errors %}<div class="status-message w-form-fail" style="display: inherit;">
          <div>{% include 'form-errors' %}</div>
        </div>{% endif %}{% if form.posted_successfully? %}<div class="status-message w-form-done" style="display: inherit;">
          <div>{{ 'collection.DIV_25' | t }}</div>
        </div>{% endif %}</div>{% endform %}
    </div>
  </div>
  <div class="section">
    <div class="container">
      <div class="w-layout-grid footer"><a href="/" id="w-node-2d9f87e2971b-87e29718" class="logo-link w-inline-block"><img src="{{ 'webflow-to-shopify.svg' | asset_url }}" width="165" alt="" class="logo-footer"></a>
        <div id="w-node-2d9f87e2971d-87e29718" class="label">{{ 'collection.DIV_26' | t }}</div>
        <div id="w-node-2d9f87e2971f-87e29718" class="links-section-footer">{% for link in linklists['footer-menu'].links %}<a href="{{ link.url }}" class="footer-link">{{ link.title }}</a>{% endfor %}</div>
        <div id="w-node-2d9f87e2972d-87e29718" class="label">{{ 'collection.DIV_27' | t }}</div>
        <div id="w-node-2d9f87e2972f-87e29718" class="links-section-footer">{% for link in linklists['footer-menu-two'].links %}<a href="{{ link.url }}" class="footer-link">{{ link.title }}</a>{% endfor %}</div>
        <div id="w-node-2d9f87e29736-87e29718" class="label">{{ 'collection.DIV_28' | t }}</div>
        <div id="w-node-2d9f87e29738-87e29718" class="links-section-footer"><a href="https://www.instagram.com/webflowapp/" target="_blank" class="footer-link">{{ 'collection.A_29' | t }}</a><a href="https://www.facebook.com/webflow/" target="_blank" class="footer-link">{{ 'collection.A_30' | t }}</a><a href="https://twitter.com/webflow" target="_blank" class="footer-link">{{ 'collection.A_31' | t }}</a></div>
      </div>
    </div>
  </div>
